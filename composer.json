{
  "name": "saparot/composer-tools",
  "description": "some tools to interact with composer tooling through php",
  "version": "0.1.0",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Saparot",
      "email": "coding@saparot.io"
    }
  ],
  "autoload": {
    "psr-4": {
      "Saparot\\ComposerTools\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Saparot\\ComposerTools\\Test\\": "tests/"
    }
  },
  "require": {
    "php": "^7.2",
    "saparot/network-retriever": "^0.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0",
    "phpunit/php-code-coverage": "^7.0",
    "phake/phake": "3.*"
  },
  "repositories": [
    {
      "type": "path",
      "url": "../network-retriever"
    }
  ]
}
